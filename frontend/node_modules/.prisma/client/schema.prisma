generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Summary {
  id            String   @id @default(cuid())
  type          String?
  status        String
  url           String?
  name          String?
  date          DateTime
  summary       String?
  sourceContent String?
}
